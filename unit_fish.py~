import pygame

class UnitFish(Sprite):
    def __init__(self, f_id = None, x_coord = None, y_coord = None):
        Sprite.__init__(self)

        self.id = f_id
        self.x = x_coord
        self.y = y_coord

        #size for
        self.size = 10 

        self.image = None

    def collided_with(self, fish):
        pass

        
    
